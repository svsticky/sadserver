# {{ ansible_managed }}

[Unit]
Description=Run intro-cs backend server
After=network.target
OnFailure=failure-notificator@%n.service

[Service]
Type=simple
User=commit
Group=commit
Restart=always

WorkingDirectory=/var/www/commit/intro-cs-backend.{{ canonical_hostname }}

ExecStart=/usr/bin/node RegistrationBackend.js
