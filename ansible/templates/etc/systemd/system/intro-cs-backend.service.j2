# {{ ansible_managed }}

[Unit]
Description=Run intro-cs backend server
After=network.target
OnFailure=failure-notificator@%n.service

[Service]
Type=simple
User=intro-cs
Group=www-data
Restart=always

WorkingDirectory=/var/www/intro-cs/backend

ExecStart=/var/www/intro-cs/.nvm/versions/node/v14.6.0/bin/node RegistrationBackend.js
