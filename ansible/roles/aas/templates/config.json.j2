	"webhookHandlers": {
		"runSystemd": [
			{
				"serviceName": "deploy-static-sticky.service",
				"endpoint": "/webhook/github",
				"pre-sharedKey": "{{ secret_static_sticky.aas_pre_shared_key }}"
			},
			{
				"serviceName": "deploy-intro-website.service",
				"endpoint": "/webhook/intro-website-deploy",
				"pre-sharedKey": "{{ secret_intro_website.aas_pre_shared_key }}"
			}
		]
	}
}
