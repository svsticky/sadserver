# {{ ansible_managed }}

[Unit]
Description=Run intro-cs backend server
After=network.target
OnFailure=failure-notificator@%n.service

[Service]
Type=simple
User=intro-cs
Group=www-data
Restart=always

WorkingDirectory=/var/www/intro-cs/intro-cs-backend.{{ canonical_hostname }}

ExecStart=/usr/bin/node RegistrationBackend.js
