# {{ ansible_managed }}

[Unit]
Description=Run the digidecs backend
OnFailure=failure-notificator@%n.service

[Service]
Type=simple
User=digidecs
Group=digidecs
Restart=always
WorkingDirectory=/var/www/digidecs/server
ExecStart=dotenv /var/www/digidecs/server/server-x86_64-unknown-linux-musl --config /var/www/digidecs/server/config.json

[Install]
WantedBy=multi-user.target
